<p id="notice"><%= notice %></p>

<p>
  <b>Phone:</b>
  <%= @sip_account.phone.mac_address %>
</p>

<p>
  <b>Name:</b>
  <%= @sip_account.name %>
</p>

<p>
  <b>Auth. user:</b>
  <%= @sip_account.auth_user %>
</p>

<p>
  <b>User:</b>
  <%= @sip_account.user %>
</p>

<p>
  <b>Password:</b>
  <%= @sip_account.password %>
</p>

<p>
  <b>Registrar:</b>
  <%= @sip_account.registrar %>
</p>

<p>
  <b>Registrar port:</b>
  <%= @sip_account.registrar_port %>
</p>

<p>
  <b>Outbound proxy:</b>
  <%= @sip_account.outbound_proxy %>
</p>

<p>
  <b>Outbound proxy port:</b>
  <%= @sip_account.outbound_proxy_port %>
</p>

<p>
  <b>SIP proxy:</b>
  <%= @sip_account.sip_proxy %>
</p>

<p>
  <b>SIP proxy port:</b>
  <%= @sip_account.sip_proxy_port %>
</p>

<p>
  <b>Realm:</b>
  <%= @sip_account.realm %>
</p>

<p>
  <b>Screen name:</b>
  <%= @sip_account.screen_name %>
</p>

<p>
  <b>Display name:</b>
  <%= @sip_account.display_name %>
</p>

<p>
  <b>Registration expiry time:</b>
  <%= @sip_account.registration_expiry_time %>
</p>

<p>
  <b>DTMF mode:</b>
  <%= @sip_account.dtmf_mode %>
</p>

<p>
  <b>Remote password:</b>
  <%= @sip_account.remote_password %>
</p>

<p>
  <b>Position:</b>
  <%= @sip_account.position %>
</p>

<div>
  <b>Codecs:</b>
<% if @sip_account.sip_account_codecs.length > 0 %>
  <ul>
<%   @sip_account.sip_account_codecs.each { |sip_account_codec| %>
    <li><%= sip_account_codec.codec.name %></li>
<%   } %>
  </ul>
<% else %>
  -
<% end %>
</div>

<p>
<%= link_to 'Create new phone key', new_sip_account_phone_key_path(@sip_account) %>
</p>
<% if @sip_account.phone_keys.count > 0 %>
<%= render :partial => 'phone_keys/table_of_phone_keys', :locals => {:sip_account => @sip_account, :phone_keys => @sip_account.phone_keys} %>
<% end %>

