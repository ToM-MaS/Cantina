<h1>Listing SIP account codecs</h1>

<table>
  <tr>
    <th>SIP account</th>
    <th>Codec</th>
    <th>Position</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sip_account_codecs.each do |sip_account_codec| %>
  <tr>
    <td><%= sip_account_codec.sip_account.name %>
      (<%= sip_account_codec.sip_account.user %>@<%= sip_account_codec.sip_account.registrar %>)</td>
    <td><%= sip_account_codec.codec.name %></td>
    <td><%= sip_account_codec.position %></td>
    <td><%= link_to 'Show', sip_account_codec %></td>
    <td><%= link_to 'Edit', edit_sip_account_codec_path(sip_account_codec) %></td>
    <td><%= link_to 'Destroy', sip_account_codec, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

